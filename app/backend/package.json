{
  "name": "@tracktor/backend",
  "version": "0.0.1",
  "description": "Tracktor backend",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc && tsc-alias",
    "dev": "tsx --watch index.ts",
    "start": "node dist/index",
    "preview": "npm run build && npm run start",
    "db:generate": "NODE_OPTIONS='--import tsx' drizzle-kit generate --custom --name",
    "db:migrate": "NODE_OPTIONS='--import tsx' drizzle-kit migrate",
    "format": "(eslint --fix . || true) && prettier --write .",
    "lint": "eslint . && prettier --check .",
    "clean": "rm -rf dist && rm -rf tracktor.db",
    "reset": "npm run clean && rm -rf tracktor.db node_modules && npm install",
    "test": "NODE_OPTIONS='--import tsx' jest --passWithNoTests"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@faker-js/faker": "^10.0.0",
    "@libsql/client": "^0.15.14",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.0",
    "drizzle-kit": "^0.31.4",
    "drizzle-orm": "^0.44.5",
    "express": "^4.21.2",
    "express-rate-limit": "^8.0.1",
    "express-validator": "^7.2.1",
    "multer": "^2.0.2",
    "sequelize": "^6.37.7",
    "sqlite3": "^5.1.7",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/jest": "^30.0.0",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.0.13",
    "@types/sqlite3": "^3.1.11",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-unused-imports": "^4.2.0",
    "jest": "^30.2.0",
    "prettier": "^3.6.2",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.4",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3"
  }
}
