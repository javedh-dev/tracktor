{
	"name": "@tracktor/frontend",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev --mode development",
		"dev:test": "vite dev --mode test",
		"build": "vite build --mode production",
		"build:dev": "vite build --mode development",
		"build:test": "vite build --mode test",
		"preview": "vite preview",
		"test": "vitest --run --mode test",
		"test:watch": "vitest",
		"test:coverage": "vitest --coverage",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "(eslint --fix . || true) && prettier --write .",
		"lint": "eslint . && prettier --check .",
		"clean": "rm -rf build .svelte-kit",
		"reset": "npm run clean && rm -rf node_modules && npm install"
	},
	"devDependencies": {
		"@eslint/js": "^9.38.0",
		"@internationalized/date": "^3.10.0",
		"@lucide/svelte": "^0.548.0",
		"@sveltejs/adapter-node": "^5.4.0",
		"@sveltejs/kit": "^2.47.3",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"@tailwindcss/forms": "^0.5.10",
		"@tailwindcss/typography": "^0.5.19",
		"@tailwindcss/vite": "^4.1.16",
		"@tanstack/table-core": "^8.21.3",
		"@testing-library/jest-dom": "^6.9.1",
		"@testing-library/svelte": "^5.2.8",
		"@typescript-eslint/eslint-plugin": "^8.46.2",
		"@typescript-eslint/parser": "^8.46.2",
		"@vitest/coverage-v8": "^4.0.3",
		"bits-ui": "^2.14.1",
		"clsx": "^2.1.1",
		"currency-codes": "^2.2.0",
		"d3-array": "^3.2.4",
		"d3-scale": "^4.0.2",
		"d3-shape": "^3.2.0",
		"date-fns": "^4.1.0",
		"date-fns-tz": "^3.2.0",
		"eslint": "^9.38.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.12.5",
		"eslint-plugin-unused-imports": "^4.3.0",
		"formsnap": "^2.0.1",
		"globals": "^16.4.0",
		"jsdom": "^27.0.1",
		"layerchart": "^2.0.0-next.27",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"prettier-plugin-tailwindcss": "^0.7.1",
		"svelte": "^5.42.2",
		"svelte-awesome-color-picker": "^4.0.2",
		"svelte-check": "^4.3.3",
		"svelte-eslint-parser": "^1.4.0",
		"svelte-sonner": "^1.0.5",
		"sveltekit-superforms": "^2.28.0",
		"tailwind-merge": "^3.3.1",
		"tailwind-variants": "^3.1.1",
		"tailwindcss": "^4.1.16",
		"tw-animate-css": "^1.4.0",
		"typescript": "^5.9.3",
		"vite": "^7.1.12",
		"vitest": "^4.0.3"
	},
	"dependencies": {
		"@types/d3-array": "^3.2.2",
		"@types/d3-scale": "^4.0.9",
		"@types/d3-shape": "^3.1.7",
		"mode-watcher": "^1.1.0",
		"svelte-loading-spinners": "^0.3.6"
	}
}
