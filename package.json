{
	"name": "tracktor",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "dotenvx run -f environment/dev.env -- vite dev --mode development",
		"dev:test": "dotenvx run -f environment/test.env -- vite dev --mode test",
		"build": "dotenvx run -f environment/prod.env -- vite build --mode production",
		"build:dev": "dotenvx run -f environment/dev.env -- vite build --mode development",
		"build:test": "dotenvx run -f environment/test.env -- vite build --mode test",
		"preview": "dotenvx run -f environment/prod.env -- vite preview",
		"test": "dotenvx run -f environment/test.env -- vitest --run --mode test",
		"test:watch": "dotenvx run -f environment/test.env -- vitest",
		"test:coverage": "dotenvx run -f environment/test.env -- vitest --coverage",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "(eslint --fix . || true) && prettier --write .",
		"lint": "eslint . && prettier --check .",
		"clean": "rm -rf build .svelte-kit *.db uploads logs coverage",
		"reset": "pnpm run clean && rm -rf node_modules && pnpm install",
		"db:generate": "dotenvx run -f environment/dev.env -- drizzle-kit generate --custom --name",
		"db:migrate": "dotenvx run -f environment/prod.env -- drizzle-kit migrate",
		"db:migrate:dev": "dotenvx run -f environment/dev.env -- drizzle-kit migrate",
		"db:migrate:test": "dotenvx run -f environment/test.env -- drizzle-kit migrate",
		"start": "dotenvx run -f environment/prod.env -- node build"
	},
	"devDependencies": {
		"@eslint/js": "^9.38.0",
		"@internationalized/date": "^3.10.0",
		"@lucide/svelte": "^0.548.0",
		"@sveltejs/adapter-node": "^5.4.0",
		"@sveltejs/kit": "^2.47.3",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"@tailwindcss/forms": "^0.5.10",
		"@tailwindcss/typography": "^0.5.19",
		"@tailwindcss/vite": "^4.1.16",
		"@tanstack/table-core": "^8.21.3",
		"@testing-library/jest-dom": "^6.9.1",
		"@testing-library/svelte": "^5.2.8",
		"@types/jest": "^30.0.0",
		"@types/node": "^24.9.1",
		"@types/supertest": "^6.0.3",
		"@typescript-eslint/eslint-plugin": "^8.46.2",
		"@typescript-eslint/parser": "^8.46.2",
		"@vitest/coverage-v8": "^4.0.3",
		"bits-ui": "^2.14.1",
		"clsx": "^2.1.1",
		"concurrently": "^9.2.1",
		"currency-codes": "^2.2.0",
		"d3-array": "^3.2.4",
		"d3-scale": "^4.0.2",
		"d3-shape": "^3.2.0",
		"date-fns": "^4.1.0",
		"date-fns-tz": "^3.2.0",
		"drizzle-kit": "^0.31.5",
		"drizzle-orm": "^0.44.7",
		"eslint": "^9.38.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.12.5",
		"eslint-plugin-unused-imports": "^4.3.0",
		"formsnap": "^2.0.1",
		"globals": "^16.4.0",
		"jest": "^30.2.0",
		"jsdom": "^27.0.1",
		"layerchart": "^2.0.0-next.27",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"prettier-plugin-tailwindcss": "^0.7.1",
		"supertest": "^7.1.4",
		"svelte": "^5.42.2",
		"svelte-awesome-color-picker": "^4.0.2",
		"svelte-check": "^4.3.3",
		"svelte-eslint-parser": "^1.4.0",
		"svelte-sonner": "^1.0.5",
		"sveltekit-superforms": "^2.28.0",
		"tailwind-merge": "^3.3.1",
		"tailwind-variants": "^3.1.1",
		"tailwindcss": "^4.1.16",
		"ts-jest": "^29.4.5",
		"tsc-alias": "^1.8.16",
