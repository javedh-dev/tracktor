{
	"name": "tracktor",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev --mode development",
		"dev:test": "vite dev --mode test",
		"build": "vite build --mode production",
		"build:dev": "vite build --mode development",
		"build:test": "vite build --mode test",
		"preview": "vite preview --mode production",
		"test": "vitest --run --mode test",
		"test:watch": "vitest --mode test",
		"test:coverage": "vitest --coverage --mode test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "(eslint --fix . || true) && prettier --write .",
		"lint": "eslint . && prettier --check .",
		"clean": "rm -rf build .svelte-kit *.db uploads logs coverage",
		"reset": "pnpm run clean && rm -rf node_modules && pnpm install",
		"db:generate": "drizzle-kit generate --custom --name",
		"db:migrate": "NODE_ENV=production drizzle-kit migrate",
		"db:migrate:dev": "drizzle-kit migrate",
		"db:migrate:test": "NODE_ENV=test drizzle-kit migrate",
		"db:seed": "tsx scripts/seed.ts",
		"db:init": "pnpm run db:migrate:dev && pnpm run db:seed",
		"start": "NODE_ENV=production node build"
	},
	"devDependencies": {
		"@eslint/js": "^9.39.1",
		"@internationalized/date": "^3.10.0",
		"@lucide/svelte": "^0.544.0",
		"@sveltejs/adapter-node": "^5.4.0",
		"@sveltejs/kit": "^2.49.2",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"@tailwindcss/forms": "^0.5.10",
		"@tailwindcss/typography": "^0.5.19",
		"@tailwindcss/vite": "^4.1.18",
		"@tanstack/table-core": "^8.21.3",
		"@testing-library/jest-dom": "^6.9.1",
		"@testing-library/svelte": "^5.2.9",
		"@types/jest": "^30.0.0",
		"@types/node": "^25.0.1",
		"@types/supertest": "^6.0.3",
		"@typescript-eslint/eslint-plugin": "^8.49.0",
		"@typescript-eslint/parser": "^8.49.0",
		"@vitest/coverage-v8": "^4.0.15",
		"bits-ui": "^2.14.4",
		"clsx": "^2.1.1",
		"concurrently": "^9.2.1",
		"currency-codes": "^2.2.0",
		"d3-array": "^3.2.4",
		"d3-scale": "^4.0.2",
		"d3-shape": "^3.2.0",
		"date-fns": "^4.1.0",
		"date-fns-tz": "^3.2.0",
		"drizzle-kit": "^0.31.8",
		"drizzle-orm": "^0.45.1",
		"eslint": "^9.39.1",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.13.1",
		"eslint-plugin-unused-imports": "^4.3.0",
		"formsnap": "^2.0.1",
		"globals": "^16.5.0",
		"jest": "^30.2.0",
		"jsdom": "^27.3.0",
		"layerchart": "^2.0.0-next.27",
		"prettier": "^3.7.4",
		"prettier-plugin-svelte": "^3.4.0",
		"prettier-plugin-tailwindcss": "^0.7.2",
		"supertest": "^7.1.4",
		"svelte": "^5.45.10",
		"svelte-awesome-color-picker": "^4.1.0",
		"svelte-check": "^4.3.4",
		"svelte-eslint-parser": "^1.4.1",
		"svelte-sonner": "^1.0.7",
		"sveltekit-superforms": "^2.28.1",
		"tailwind-merge": "^3.4.0",
		"tailwind-variants": "^3.2.2",
		"tailwindcss": "^4.1.18",
		"ts-jest": "^29.4.6",
		"tsc-alias": "^1.8.16",
		"tsc-esm-fix": "^3.1.2",
		"tsx": "^4.21.0",
		"tw-animate-css": "^1.4.0",
		"typescript": "^5.9.3",
		"vite": "^7.2.7",
		"vitest": "^4.0.15"
	},
	"dependencies": {
		"@dotenvx/dotenvx": "^1.51.1",
		"@faker-js/faker": "^10.1.0",
